<nav>

    {% set navPages = collections.all | eleventyNavigation %}

    <ul>

        {%- for entry in navPages %}
            <li>
                <a href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item"{% endif %}>
                    {{ entry.title }}
                </a>
            </li>
        {%- endfor %}

        <li>
            {% include "components/btn-color-scheme.njk" %}
        </li>

    </ul>

</nav>
